{
	"schemaVersion": "0.1.0",
	"name": "IMDB Movie",
	"behavior": "create",
	"noteContentFormat": "# Summary\n\n{{schema:description}}",
	"properties": [
		{
			"name": "cover",
			"value": "{{schema:image}}",
			"type": "text"
		},
		{
			"name": "date",
			"value": "",
			"type": "date"
		},
		{
			"name": "director",
			"value": "{{schema:director[*].name|wikilink|join}}{{schema:creator[*].name|wikilink|join}}",
			"type": "multitext"
		},
		{
			"name": "genres",
			"value": "{{schema:genre|join}}",
			"type": "multitext"
		},
		{
			"name": "icon",
			"value": "lucide-clapperboard",
			"type": "text"
		},
		{
			"name": "lists",
			"value": "",
			"type": "multitext"
		},
		{
			"name": "status-movie",
			"value": "",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "movie",
			"type": "multitext"
		},
		{
			"name": "year",
			"value": "{{schema:datePublished|split:\\\"-\\\"|slice:0,1}}",
			"type": "number"
		}
	],
	"triggers": [
		"schema:@Movie"
	],
	"noteNameFormat": "{{schema:name|replace:\":\":\"êž‰\"}}",
	"path": "Objects/Movies"
}