{
	"schemaVersion": "0.1.0",
	"name": "IMDB TV Series",
	"behavior": "create",
	"noteContentFormat": "# Summary\n\n{{schema:description}}",
	"properties": [
		{
			"name": "genres",
			"value": "{{schema:genre|join}}",
			"type": "multitext"
		},
		{
			"name": "year",
			"value": "{{schema:datePublished|split:\\\"-\\\"|first}}",
			"type": "number"
		},
		{
			"name": "cover",
			"value": "{{schema:image}}",
			"type": "text"
		},
		{
			"name": "lists",
			"value": "{{schema:name|replace:\\\":\\\":\\\"꞉\\\"|wikilink}}",
			"type": "multitext"
		},
		{
			"name": "tv-status",
			"value": "",
			"type": "text"
		},
		{
			"name": "total-episodes",
			"value": "{{selector:.ipc-title[data-testid=\\\"episodes-header\\\"] .ipc-title__subtext}}",
			"type": "number"
		},
		{
			"name": "watched-episodes",
			"value": "0",
			"type": "number"
		},
		{
			"name": "date",
			"value": "",
			"type": "date"
		},
		{
			"name": "tags",
			"value": "tv-series",
			"type": "multitext"
		},
		{
			"name": "icon",
			"value": "lucide-tv",
			"type": "text"
		}
	],
	"triggers": [
		"schema:@TVSeries"
	],
	"noteNameFormat": "{{schema:name|replace:\":\":\"꞉\"}}",
	"path": "Objects/TV Shows"
}