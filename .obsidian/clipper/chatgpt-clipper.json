{
	"schemaVersion": "0.1.0",
	"name": "ChatGPT",
	"behavior": "create",
	"noteContentFormat": "{{selectorHtml:article[data-testid*=\"conversation-turn\"]|join:\"LINEBREAKPLACEHOLDER\"|replace:\"/<svg[^>]*>.*?<\\\\/svg>/gs\":\"\"|remove_html:(\".text-token-text-secondary,.text-token-text-tertiary\")|markdown|replace:\"##### You said:\\n\":\"[!user]\"|replace:\"###### ChatGPT said:\\n\":\"[!openai]\"|replace:\"/(?<=\\s|^)#(\\w+)/g\":\"\\\\#$1\"|replace:\"/<([^>]+)>/g\":\"\\\\<$1\\\\>\"|blockquote|replace:\"> \\n> LINEBREAKPLACEHOLDER\\n> \":\"\"}}",
	"properties": [
		{
			"name": "created",
			"value": "{{time|date:\\\"YYYY-MM-DDTHH:mm:ss.SSSZ\\\"}}",
			"type": "date"
		},
		{
			"name": "source",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "clippings, chatgpt",
			"type": "multitext"
		},
		{
			"name": "icon",
			"value": "lucide-bot-message-square",
			"type": "text"
		}
	],
	"triggers": [
		"https://chatgpt.com/"
	],
	"noteNameFormat": "{{selector:title|safe_name}} - ChatGPT",
	"path": "Objects/ChatGPT"
}